bin = bcp cat chmod chown clear date del df dmesg du ff fn locfg \
	ls ren stat sync symlink sysinfo time which whoami tail

man1 = bcp.1 cat.1 chmod.1 chown.1 clear.1 date.1 del.1 df.1 du.1 \
	ff.1 fn.1 ls.1 locfg.1 ren.1 stat.1 sync.1 symlink.1 time.1 \
	which.1 tail.1

include ../rules.mk

date: date.o date_find.o date_time.o
